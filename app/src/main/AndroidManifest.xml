<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="anwarabdullahn.com.villacorp_apps">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name=".Initialize"
            android:allowBackup="true"
            android:icon="@mipmap/ic_apps"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_apps_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".Activity.DashboardActivity"
                android:label="@string/title_activity_dashboard">
        </activity>
        <activity
                android:name=".Activity.PesanDetailActivity"
                android:theme="@style/AppTheme">
        </activity>
        <activity
                android:name=".Activity.TukarLibur.TukarLiburActivity"
                android:label="@string/title_activity_tukar_libur"
                android:theme="@style/AppTheme">
        </activity>
        <activity
                android:name=".Activity.TukarLibur.DoTukarLiburActivity"
                android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".Activity.TukarLibur.DoTukarLiburDetail">
        </activity>
        <activity android:name=".Activity.InOut.InOutActivity">
        </activity>
        <activity android:name=".Activity.InOut.DoInOutActivity">
        </activity>
    </application>

</manifest>